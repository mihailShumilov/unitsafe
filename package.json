{
  "name": "unitsafe",
  "version": "0.1.0",
  "description": "Compile-time dimension safety for TypeScript with zero runtime cost",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "type-tests": "tsd",
    "bench": "tsx bench/index.ts",
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "typescript",
    "units",
    "dimensions",
    "type-safety",
    "zero-cost",
    "physics",
    "si-units"
  ],
  "author": {
    "name": "Mykhailo Shumilov",
    "email": "michaelfromua@gmail.com",
    "url": "https://www.linkedin.com/in/michael-shumilov-46453737/"
  },
  "license": "MIT",
  "devDependencies": {
    "tsd": "^0.33.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "tsd": {
    "directory": "type-tests"
  },
  "engines": {
    "node": ">=20"
  }
}
